#include <iostream>
using namespace std;

void printArray(int a[], int size){
    int i;
    for(i=0;i<size;i++){
        cout << a[i] << endl;
    }
}

int sumArray(int a[], int size){
    int i;
    int sum=0;
    for(i=0;i<size;i++){
        sum=sum+a[i];
    }
    return sum;
}
int main() {
    int a[50];
    int size;
    int i;
    cout << "Enter the size: " << endl;
    cin >> size;
    
    cout << "Enter number of element: " << endl; 
    for(i=0;i<size;i++){
        cin >> a[i];
    }
    
    //printArray(a,size);
    
    int sum=sumArray(a,size);
    cout << "The sum of all element in array is:" << sum <<endl;
    return 0;
}
