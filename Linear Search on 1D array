#include <iostream>
using namespace std;

int main(){
    int arr[100];
    int size;
    cout << "Enter the size of array: " << endl;
    cin >> size;
    
    cout << "Enter the elements in array: " << endl;
    for(int i=0; i<size; i++){
        cin >> arr[i];
    }
    int key;
    cout << "Enter the key element: "<< endl;
    cin >> key;
    
    for(int i=0; i<size; i++){
        if(arr[i]==key){
            cout<< "Key element: " << key << " found at index: " << i << endl;
        }
    }
}
