#include <iostream>
using namespace std;

void printArray(int a[], int size){
    for(int i = 0; i < size; i++){
        cout << a[i] << " ";
    }
    cout << endl;
}

void swap(int &i, int &j){
    int temp = i;
    i = j;
    j = temp;
}

void rev_Array(int a[], int size){
    int start = 0;
    int end = size - 1;

    while(start < end){
        swap(a[start], a[end]);
        start++;
        end--;
    }
}

int main() {
    int a[50];
    int size;

    cout << "Enter the size: " << endl;
    cin >> size;

    cout << "Enter number of elements: " << endl; 
    for(int i = 0; i < size; i++){
        cin >> a[i];
    }

    rev_Array(a, size);
    printArray(a, size);

    return 0;
}
