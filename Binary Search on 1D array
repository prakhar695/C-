#include<iostream>
using namespace std;

int main(){
    int found=0;
    int a[20];
    int n;
    cout << "Enter the size of array: " << endl;
    cin >> n;
    
    cout << "Enter the element in array: " << endl;
    for(int i=0; i<n; i++){
        cin >> a[i];
    }
    
    int key;
    cout << "Enter the key element: " << endl;
    cin >> key;
    
    int start = 0;
    int end = n-1;
    int mid=(start+end)/2;
    while(start<=end){
        mid=(start+end)/2;
        if(a[mid]==key){
            found = 1;
            break;
        }
        if(a[mid]<key){
            start=mid+1;
        }
        else{
            end=mid-1;
        }
    }
    
    if(found==1){
        cout << "Element present in array at index: " << mid << endl;
    }
    else{
        cout << "Element not present in array" << endl;
    }
}
